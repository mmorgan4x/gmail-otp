{
    "manifest_version": 3,
    "name": "OTP Scanner for Gmail",
    "version": "1.1",
    "description": "Scan Gmail for one-time-passwords for quick and easy copy/paste without leaving the website.",
    "icons": {
        "16": "assets/icon.png",
        "48": "assets/icon.png",
        "128": "assets/icon.png"
    },
    "action": {
        "default_popup": "/index.html#/popup",
        "default_icon": "assets/icon.png"
    },
    "options_page": "/index.html#/options",
    "permissions": [
        "activeTab",
        "identity",
        "identity.email"
    ],
    "key": "",
    "oauth2": {
        "client_id": "732667058697-p4s4cb64l0rdjdbtd0enmd01a1lvimmn.apps.googleusercontent.com",
        "scopes": [
            "https://www.googleapis.com/auth/gmail.readonly",
            "https://www.googleapis.com/auth/userinfo.profile"
        ]
    }
}